{
  "expo": {
    "newArchEnabled": true,
    "name": "vigormancer",
    "slug": "vigormancer",
    "version": "1.0.0",
    "scheme": "vigormancer",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [["expo-router"], "expo-localization", "expo-font"],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.ranimontagna.vigormancer"
    }
  }
}
